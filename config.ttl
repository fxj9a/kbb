conf:store a <Kanban-Board-Repo-Store> ;
    <pathOfGitRepo> "https://github.com/fxj9a/kbb.git/store" ; # Set the path to the repository that contains the files .
    <origin> "https://github.com/fxj9a/kbb.git" . # Optional a git repo that will be cloned into dir given in line above on startup.


conf:example a <Graph> ; # Define a Graph resource for a named graph
    <graphUri> <https://github.com/fxj9a/kbb/tree/master/store> ; # Set the URI of named graph
    <isVersioned> 1 ; # Defaults to True, future work
    <graphFile> "file.nq" . # Set the filename
